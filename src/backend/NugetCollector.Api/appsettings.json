{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "CorsOrigin": "http://localhost:8080",
  "CollectorOptions": {
    "LocalPath": "[WORKING_DIRECTORY]",
    "ViaHttp": false,
    "Credential": {
      "Username": "[GIT_USERNAME]",
      "Password": "[GIT_PASSWORD]",
      "Email": "[GIT_EMAIL]"
    },
    "Repositories": [
      {
        "Name": "[GIT_REPOSITORY_NAME]",
        "Path": "[GIT_REPOSITORY_PATH]"
      }
    ]
  }
}
